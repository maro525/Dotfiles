source ~/.zshrc_manjaro_default

# alias
alias pacmanupdate="sudo pacman -Syu"
alias yayupdate="yay -Scc && yay -Syu"
alias managerupdate="yay -S yay -d"
alias pacmanclean="sudo pacman -Rs $(pacman -Qdtq)"
alias yayclean="yay -Yc"
alias yayreinstallall="yay -Qnq | yay -S -"

alias ls="ls -a"
alias drop="cd ~/Dropbox"

export XDG_CONFIG_HOME=~/.config
export TERM=xterm-256color

export PATH="$HOME/.cargo/bin:$PATH"
